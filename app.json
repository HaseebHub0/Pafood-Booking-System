{
  "expo": {
    "name": "PAFood Booking",
    "slug": "pafood-booking-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "scheme": "pafood",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#DC2626"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.pafood.booking",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "This app needs location access to track shop visits and deliveries.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "This app needs location access to track your route and enable real-time monitoring."
      },
      "config": {
        "googleMapsApiKey": "AIzaSyDjzAxNJfhgWUHYclJrftOffTweT5hirhI"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#DC2626"
      },
      "package": "com.pafood.booking",
      "edgeToEdgeEnabled": true,
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "ACCESS_BACKGROUND_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "ACCESS_BACKGROUND_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ],
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyDjzAxNJfhgWUHYclJrftOffTweT5hirhI"
        }
      }
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow PAFood Booking to use your location to track your route and enable real-time monitoring."
        }
      ],
      [
        "react-native-maps",
        {
          "googleMapsApiKey": "AIzaSyDjzAxNJfhgWUHYclJrftOffTweT5hirhI"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "fea648b8-04c9-4a91-aeb4-a3fcd5a6b55b"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/fea648b8-04c9-4a91-aeb4-a3fcd5a6b55b",
      "enabled": true,
      "checkAutomatically": "ON_LOAD",
      "fallbackToCacheTimeout": 0
    }
  }
}
